{% extends 'base.html.twig' %}
{% block title %}View Question{% endblock %}
{% block body %}
   <h3 class="text-center"> {{ note.noteName }}</h3>
   {% if note.body %}
   <div class="row">
       <div class="col-8 offset-2">
       <div class="card">
           <div class="card-body">
               <p class="card-text">{{ note.body }}</p>
           </div>
           <div class="card-footer text-muted small">
            {{ note.created|date }}
           </div>
       </div>
       </div>
   </div>
   {% endif %}
{% endblock %}